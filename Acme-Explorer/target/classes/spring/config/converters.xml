<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- 
			Add your own converters using entries of the following form: 
			<bean class="converters.Class1ToClass2" /> 
		-->
		
		<bean class="converters.AuthorityToStringConverter"/>
		<bean class="converters.StringToAuthorityConverter"/>
		
		<bean class="converters.ContactToStringConverter"/>
		<bean class="converters.StringToContactConverter"/>
		
		<bean class="converters.CurriculumToStringConverter"/>
		<bean class="converters.StringToCurriculumConverter"/>
		
		<bean class="converters.ExplorerToStringConverter"/>
		<bean class="converters.StringToExplorerConverter"/>
		
		<bean class="converters.FolderToStringConverter" />
		<bean class="converters.StringToFolderConverter" />
		
		<bean class="converters.MessageToStringConverter" />
		<bean class="converters.StringToMessageConverter" />
		
		<bean class="converters.SocialIDToStringConverter"/>
		<bean class="converters.StringToSocialIDConverter"/>
		
		<bean class="converters.StoryToStringConverter"/>
		<bean class="converters.StringToStoryConverter"/>
		
		<bean class="converters.SurvivalClassToStringConverter"/>
		<bean class="converters.StringToSurvivalClassConverter"/>
		
		<bean class="converters.TripToStringConverter"/>
		<bean class="converters.StringToTripConverter"/>
		
		<bean class="converters.TripApplicationToStringConverter"/>
		<bean class="converters.StringToTripApplicationConverter"/>
	</util:list>
	
</beans>